{
	"name": "mati-core/sandbox",
	"description": "Mati-Core nette sandbox",
	"homepage": "https://nette.org",
	"type": "project",
	"license": ["BSD-3-Clause", "GPL-2.0-only", "GPL-3.0-only"],
	"authors": [
		{
			"name": "David Grudl",
			"homepage": "https://davidgrudl.com"
		},
		{
			"name": "Nette Community",
			"homepage": "https://nette.org/contributors"
		},
		{
			"name": "Ing. Martin Olmr",
			"homepage": "https://martinolmr.cz"
		}
	],
	"require": {
		"php": ">=8.0",
		"nette/application": "^3.1",
		"nette/bootstrap": "^3.1",
		"nette/caching": "^3.1",
		"nette/database": "^3.1",
		"nette/di": "^3.0",
		"nette/finder": "^2.5",
		"nette/forms": "^3.1",
		"nette/http": "^3.1",
		"nette/mail": "^3.1",
		"nette/robot-loader": "^3.3",
		"nette/safe-stream": "^2.4",
		"nette/security": "^3.1",
		"nette/utils": "^3.2",
		"latte/latte": "^2.9",
		"tracy/tracy": "^2.8",
		"baraja-core/package-manager": "~2.0",
		"baraja-core/doctrine": "~3.0",
		"contributte/translation": "^0.8.3",
		"mati-core/email": "^1.0",
		"mati-core/user": "^1.0",
		"mati-core/form": "^1.0",
		"mati-core/cms": "^1.0"
	},
	"require-dev": {
		"roave/security-advisories": "dev-master",
		"nette/tester": "^2.3",
		"phpstan/phpstan-nette": "^0.12",
		"symfony/thanks": "^1"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app"
		}
	},
	"minimum-stability": "stable",
	"scripts": {
		"phpstan": "phpstan analyse --configuration phpstan.neon app www",
		"post-autoload-dump": "Baraja\\PackageManager\\PackageRegistrator::composerPostAutoloadDump"
	}
}
